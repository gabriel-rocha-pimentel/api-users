{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "node_modules/.prisma/**/*"
          ]
        },
        "env": {
            "NODE_ENV": "production"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }
  