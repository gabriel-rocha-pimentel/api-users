{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/**/*"]
      }
    }
  ],
  "hooks": {
    "build": "npm run build"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./server.js"
    }
  ]
}
